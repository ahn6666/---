page {
  background: #dedede;
}
.search_row {
  display: flex;
  height: 70rpx;
}
.search_row input {
  flex: 7;
  border: 2rpx solid white;
  height: 100%;
  background: white;
  margin: 10rpx ;
}
.search_row button {
  flex: 1;
  height: 100%;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 10rpx;
}
.search_content {
  margin-top: 20rpx;
}
.search_content .content_info {
  height: 60rpx;
  border-bottom: 2rpx solid white;
  background: white;
  width: 100%;
  margin-bottom: 10rpx;
  padding: 10rpx 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
