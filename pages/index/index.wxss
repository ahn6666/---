/**index.wxss**/
.index_swiper swiper {
  width: 760rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
  padding: 20rpx;
}
.index_cate navigator {
  flex: 1;
}
.index_cate navigator image {
  width: 100%;
}
.index_floort {
  padding: 10rpx;
}
.index_floort .floor_title image {
  width: 100%;
}
.index_floort .floor_list {
  overflow: hidden;
}
.index_floort .floor_list navigator {
  float: left;
  width: 33.33%;
}
.index_floort .floor_list navigator:nth-child(2) {
  border-bottom: 10rpx solid #fff;
}
.index_floort .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floort .floor_list navigator:nth-last-child(-n+4) {
  /* 
            232 *382
            原图的高度96*162
            232*386=33.33vw*162/92

            */
  height: 27.43978448vw;
  border-left: 10rpx solid #fff;
}
.index_floort .floor_list navigator image {
  width: 100%;
  height: 100%;
}
