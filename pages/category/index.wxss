pages {
  height: 100%;
}
.catas {
  height: 100%;
}
.catas .catas_index {
  height: calc(100hv - 90rpx);
  display: flex;
}
.catas .catas_index .leftMune {
  flex: 2;
}
.catas .catas_index .leftMune view {
  height: 80rpx;
  justify-content: center;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.catas .catas_index .active {
  color: var(--themecolor);
  border-left: 5px solid currentColor;
}
.catas .catas_index .rightMune {
  flex: 5;
}
.catas .catas_index .rightMune .right_index .good_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.catas .catas_index .rightMune .right_index .good_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.catas .catas_index .rightMune .right_index .good_image {
  display: flex;
  flex-wrap: wrap;
}
.catas .catas_index .rightMune .right_index .good_image navigator {
  width: 33.33%;
  text-align: center;
}
.catas .catas_index .rightMune .right_index .good_image navigator image {
  width: 50%;
}
